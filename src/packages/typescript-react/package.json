{
  "$schema": "https://json.schemastore.org/package",
  "name": "@fundamentry/eslint-config-typescript-react",
  "version": "1.0.0-x",
  "description": "TypeScript React ESLint configuration.",
  "repository": {
    "type": "git",
    "url": "git@github.com:fundamentry/eslint-config.git"
  },
  "contributors": [
    {
      "name": "Alexander Zakharov (azaxarov)",
      "email": "alexander.zakharov@fundamentry.org"
    }
  ],
  "engines": {
    "node": ">=24",
    "pnpm": ">=10"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "run-s clean compile",
